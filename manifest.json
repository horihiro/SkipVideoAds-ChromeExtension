{
  "manifest_version": 3,
  "name": "Skip Video Ads",
  "short_name": "Skip Video Ads",
  "version": "0.0.1",
  "version_name": "0.0.1-abc",
  "description": "A Chrome extension that skips video ads.",
  "permissions": [
  ],
  "action": {
    "default_icon": "img/icon128.png"
  },
  "icons": {
    "128": "img/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.amazon.co.jp/gp/video/*",
        "https://tver.jp/episodes/*"
      ],
      "js": [
        "dist/contentScript/main.js"
      ]
    }
  ],
  "background": {
  }
}
